#include "stdafx.h"
#include <iostream>
using namespace std;


int main()
{
	int n;
	int e;
	cout << "please enter a number" << endl;

	cin >> n;
	if (n < 7)
	{
		cout << "lower than 7" << endl;
	}
	else if (n == 7)
	{
		cout << "it is 7" << endl;
	}
	else
	{
		cout << "more than 7" << endl;
	}
	cout << "your number was: " << n << endl;
    return 0;
}
